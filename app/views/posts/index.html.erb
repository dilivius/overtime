<h3>Posts</h3>


<table class="table table-hover table-striped">
  <thead>
    <tr>
      <th>
        #
      </th>
      <th>
        Hours
      </th>
      <th>
        Date
      </th>
      <th>
        Work performed
      </th>
      <th>
        User
      </th>
      <th>
        Status
      </th>
      <th>
      </th>
    </tr>
  </thead>
  <tbody id="posts">
    <%= render @posts %>
  </tbody>
</table>

<div id="paginator">
  <%= paginate @posts, remote: true %>
</div>
